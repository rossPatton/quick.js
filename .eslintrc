{
	"ecmaFeatures": {
		"arrowFunctions": true,
		"binaryLiterals": true,
		"blockBindings": true,
		"classes": true,
		"defaultParams": true,
		"destructuring": true,
		"forOf": true,
		"generators": true,
		"modules": true,
		"objectLiteralComputedProperties": true,
		"objectLiteralDuplicateProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"octalLiterals": true,
		"regexYFlag": true,
		"restParams": true,
		"spread": true,
		"templateStrings": true,
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"brace-style": [2,
			"stroustrup", {
				"allowSingleLine": true
			}
		],
		"camelcase": 1,
		"comma-dangle": 1,
		"comma-spacing": [1, {
			"before": false,
			"after": true
		}],
		"comma-style": [2, "last"],
		"consistent-return": 1,
		"consistent-this": 0,
		"curly": 0,
		"default-case": 1,
		"eol-last": 0,
		"eqeqeq": 1,
		"func-names": 0,
		"func-style": 0,
		"generator-star": 1,
		"guard-for-in": 1,
		"key-spacing": 0,
		"max-nested-callbacks": 0,
		"new-cap": 0,
		"no-constant-condition": 1,
		"no-control-regex": 1,
		"new-parens": 0,
		"no-alert": 0,
		"no-array-constructor": 1,
		"no-caller": 0,
		"no-catch-shadow": 0,
		"no-cond-assign": 1,
		"no-delete-var": 1,
		"no-div-regex": 0,
		"no-dupe-args": 1,
		"no-dupe-keys": 1,
		"no-else-return": 1,
		"no-empty": 1,
		"no-empty-label": 0,
		"no-eq-null": 1,
		"no-eval": 1,
		"no-ex-assign": 0,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": 1,
		"no-extra-semi": 1,
		"no-fallthrough": 0,
		"no-floating-decimal": 0,
		"no-func-assign": 1,
		"no-implied-eval": 1,
		"no-inline-comments": 1,
		"no-inner-declarations": 1,
		"no-invalid-regexp": 1,
		"no-irregular-whitespace": 1,
		"no-iterator": 0,
		"no-label-var": 1,
		"no-labels": 1,
		"no-lone-blocks": 1,
		"no-loop-func": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-multi-spaces": 0,
		"no-multi-str": 1,
		"no-native-reassign": 1,
		"no-negated-in-lhs": 1,
		"no-nested-ternary": 1,
		"no-new": 0,
		"no-new-func": 0,
		"no-new-object": 0,
		"no-new-wrappers": 0,
		"no-obj-calls": 1,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-proto": 0,
		"no-redeclare": 0,
		"no-regex-spaces": 0,
		"no-reserved-keys": 0,
		"no-return-assign": 0,
		"no-script-url": 0,
		"no-self-compare": 0,
		"no-sequences": 0,
		"no-shadow": 0,
		"no-shadow-restricted-names": 1,
		"no-spaced-func": 1,
		"no-sparse-arrays": 1,
		"no-trailing-spaces": [2, {
			"skipBlankLines": true
		}],
		"no-undef": 1,
		"no-undef-init": 1,
		"no-undefined": 1,
		"no-underscore-dangle": 0,
		"no-unreachable": 1,
		"no-unused-expressions": 1,
		"no-unused-vars": 1,
		"no-use-before-define": 1,
		"no-var": 1,
		"no-void": 1,
		"no-warning-comments": 0,
		"no-with": 1,
		"no-wrap-func": 0,
		"one-var": [2, "never"],
		"operator-assignment": 1,
		"padded-blocks": 0,
		"quote-props": 0,
		"quotes": [2, "single", "avoid-escape"],
		"radix": 1,
		"semi": 0,
		"sort-vars": 0,
		"space-after-function-name": 0,
		"space-after-keywords": 1,
		"space-before-blocks": 1,
		"space-in-brackets": 0,
		"space-in-parens": [2, "always"],
		"space-infix-ops": 1,
		"space-return-throw-case": 1,
		"space-unary-ops": 1,
		"spaced-line-comment": 1,
		"use-isnan": 0,
		"valid-jsdoc": 1,
		"valid-typeof": 1,
		"vars-on-top": 0,
		"wrap-iife": [2, "inside"],
		"yoda": [2, "never"]
	},
	"plugins": [
		"babel"
	]
}
